<!-- Move Reviews under related products -->
<script>
  jQuery(document).ready(function($){
          setTimeout(function(){
	$('section.reviewsSection').insertAfter('article.ProductItem>section:has(.ProductItem-related-label)');
          }, 3000);
      })
</script>

